package lab1;

public class Rectangle extends Shape{
	private int height;
	private int width;

	Rectangle(int height, int width) {
		super(4);
		this.height= height;
		this.width= width;
	}
	
	public void setHeight(int height) {
		this.height= height;
	}
	public void setWidth(int width) {
		this.width= width;
	}
	
	public int getHeight() {
		return height;
	}
	
	public int getWidth() {
		return width;
	}
	
	@Override
	public int getArea() {
		return height* width;
	}
	
	@Override
	public String toString() {
		return "area= "+getArea();
	}
}
