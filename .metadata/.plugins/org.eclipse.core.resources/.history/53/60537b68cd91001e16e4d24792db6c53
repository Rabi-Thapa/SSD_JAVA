package lab7;

import java.util.HashSet;
import java.util.Set;

public class EmailStore {
	Set<String> emailAddress = new HashSet<>();
	
	

	public int findEmail(String email) {
		for (int i = 0; i < emailAddress.size(); i++) {
			if (emailAddress.contains(email)) {
				return i;
			}
		}
		return -1;
	}

}
