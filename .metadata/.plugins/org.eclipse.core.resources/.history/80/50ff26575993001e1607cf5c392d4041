package lab1;

public class Ellipse extends Rectangle{
	private int x_axis;
	private int y_axis;
	
	public Ellipse(int x_axis, int y_axis) {
		super(0,0);
		this.x_axis= x_axis;
		this.y_axis= y_axis;
		
	}
	@Override
	public double getArea() {
		return Math.PI * x_axis * y_axis;
	}
	
	@Override
	public String toString() {
		return "Ellipse [x_axis=" + x_axis + ", y_axis=" + y_axis + ", sides="+getSides()+", area="+getArea()+ "]";
	}
	
}
