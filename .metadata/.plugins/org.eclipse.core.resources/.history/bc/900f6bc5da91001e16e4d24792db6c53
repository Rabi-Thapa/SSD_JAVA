package lab7;



public class Driver {
	public static void main(String[] args) {
		EmailStore es= new EmailStore();
		es.addEmail("rabi@gmail.com");
		es.addEmail("thapa@gmail.com");
		es.addEmail("magar@gmail.in");
		
		System.out.println(es.hasEmail("rabi@gmail.com"));
		System.out.println(es.hasEmail("hululu@gmail.com"));
		System.out.println(es.addEmail("thapa@gmail.com"));
		System.out.println(es.addEmail("abcde@tbc.in"));
		es.displayEmails();
	}
}
