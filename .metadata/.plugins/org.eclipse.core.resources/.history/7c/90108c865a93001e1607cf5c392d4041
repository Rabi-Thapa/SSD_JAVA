package lab1;

public class Ellipse extends Rectangle{
	private int height;
	private int width;
	
	public Ellipse() {
		super(1);
	}
	
	public void setHeight(int height) {
		this.height= height;
	}
	public void setWidth(int width) {
		this.width= width;
	}
	@Override
	public double getArea() {
		return Math.PI * height * width;
	}
	
	@Override
	public String toString() {
		return "Ellipse [height=" + height + ", width=" + width + ", sides="+getSides()+", area="+getArea()+ "]";
	}
	
}
