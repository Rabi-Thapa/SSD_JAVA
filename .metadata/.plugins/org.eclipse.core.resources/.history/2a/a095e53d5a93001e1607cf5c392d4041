package lab1;

public class Ellipse extends Rectangle{
	private int height;
	private int width;
	
	public Ellipse() {
		super(1);
	}
	@Override
	public double getArea() {
		return Math.PI * height * width;
	}
	
	@Override
	public String toString() {
		return "Ellipse [x_axis=" + height + ", y_axis=" + width + ", sides="+getSides()+", area="+getArea()+ "]";
	}
	
}
